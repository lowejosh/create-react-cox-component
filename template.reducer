import { Reducer } from "react";
import { COMPONENT_NAMEState, COMPONENT_NAMEAction } from "./COMPONENT_NAME.types";

export const COMPONENT_NAMEInitialState: COMPONENT_NAMEState = {
  foo: true,
};

export const COMPONENT_NAMEReducer: Reducer<COMPONENT_NAMEState, COMPONENT_NAMEAction> = (
  state,
  action
) => {
  switch (action.type) {
    case "SET_STATE":
      return { ...state, ...action.payload };
    default:
      throw new Error("An invalid action type was used");
  }
};
